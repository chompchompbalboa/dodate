{
    "name": "dodate",
    "private": true,
    "main": "resources/js/bundles/native/ExpoEntry.js",
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --https --key ~/.config/valet/Certificates/dodate.test.key --cert ~/.config/valet/Certificates/dodate.test.crt --config=node_modules/laravel-mix/setup/webpack.config.js --disable-host-check",
        "hot-production": "cross-env NODE_ENV=production node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js --disable-host-check",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "test": "jest  --config=jest.config.web.js --passWithNoTests",
        "test-watch": "jest  --config=jest.config.web.js --watch",
        "test-native": "jest  --config=jest.config.native.js --passWithNoTests",
        "test-native-watch": "jest  --config=jest.config.native.js --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
